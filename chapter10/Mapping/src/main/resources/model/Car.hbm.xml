<hibernate-mapping>
    <class name="model.Car" table="cars">
        <id name="id" column="id">
            <generator class="identity" />
        </id>
        <many-to-one name="model" column="model_id" class="model.Model" lazy="false"/>
        <many-to-one name="body" column="body_id" class="model.Body" lazy="false"/>
        <many-to-one name="producer" column="producer_id" class="model.Producer" lazy="false" />
    </class>
</hibernate-mapping>