<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>ru.evrnsky.chapter8.Parser</groupId>
	<artifactId>Parser</artifactId>
	<name>Parser</name>
	<version>1.0</version>
	<packaging>jar</packaging>
	
	<parent>
		<groupId>ru.evrnsky.chapter8</groupId>
		<artifactId>chapter8</artifactId>
